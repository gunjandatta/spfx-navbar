{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "nav-bar-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/navBar/NavBarApplicationCustomizer.js",
          "manifest": "./src/extensions/navBar/NavBarApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "gd-sprest-bs": {
      "path": "node_modules/gd-sprest-bs/dist/gd-sprest-bs.min.js",
      "globalName": "$REST"
    }
  },
  "localizedResources": {
    "NavBarApplicationCustomizerStrings": "lib/extensions/navBar/loc/{locale}.js"
  }
}